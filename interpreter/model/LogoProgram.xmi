<?xml version="1.0" encoding="UTF-8"?>
<ASM:LogoProgram
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ASM="http://www.kermeta.org/kmLogo/ASM"
    xsi:schemaLocation="http://www.kermeta.org/kmLogo/ASM ../../k3.sample.maven.logo.model/model/ASMLogo.ecore#//ASM">
  <instructions
      xsi:type="ASM:ProcDeclaration"
      name="Square"
      procCall="//@instructions.1">
    <args name="size"/>
    <instructions
        xsi:type="ASM:Block">
      <instructions
          xsi:type="ASM:PenDown"/>
      <instructions
          xsi:type="ASM:Repeat">
        <condition
            xsi:type="ASM:Constant"
            integerValue="4"/>
        <block>
          <instructions
              xsi:type="ASM:Forward">
            <steps
                xsi:type="ASM:ParameterCall"
                parameter="//@instructions.0/@args.0"/>
          </instructions>
          <instructions
              xsi:type="ASM:Left">
            <angle
                xsi:type="ASM:Constant"
                integerValue="90"/>
          </instructions>
        </block>
      </instructions>
      <instructions
          xsi:type="ASM:PenUp"/>
    </instructions>
  </instructions>
  <instructions
      xsi:type="ASM:ProcCall"
      declaration="//@instructions.0">
    <actualArgs
        xsi:type="ASM:Constant"
        integerValue="15"/>
  </instructions>
</ASM:LogoProgram>
